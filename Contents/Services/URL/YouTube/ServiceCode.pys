def NormalizeURL(url):	#Log("*********** In YouTube normalizeURL")		# Deal with special providerInfo URL built up by plugin to return	# info about this provider. For all other normal URLs, do nothing. 	if ("providerinfo" in url):			try:			show = Prefs["show_youtube"]		except Exception, ex:			show = True				if (show):			return url + "&supported=true"		else:			return url				else:		return url	